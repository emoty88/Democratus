<IfModule mod_rewrite.c>

    RewriteEngine On
    RewriteBase /

    # RewriteCond %{HTTP_HOST} ^cafeincampus.org [NC]
    # RewriteCond %{HTTP_HOST} !^cafeincampus.org [NC]

    # RewriteRule ^(.*)$ http://democ.com%{REQUEST_URI} [R=301,L]
    
    
    RewriteCond %{HTTP_HOST} !^cafeincampus.org$ [NC]
    RewriteRule ^(.*)$ http://cafeincampus.org/$1 [R=301,L]

    

    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-l
    RewriteRule .* index.php [L]
    
</IfModule>










